<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
	function Person(name, phone,addr){
		this.name =name
		this.phone=phone
		this.addr=addr
	}
	
	Person.prototype.hobby = '영화감상'
	Person.prototype.info = function(){
		let msg =''
		for (let key in this ) {//내객체안의 모든 키를 보고 추출
			if(key!='info')
			msg +=key + ':' +this[key]+'\n'
		}

	alert(msg)
	}

	let p = new Person("홍길동", '010-2200-4444', '경기도')
	p.info()
	console.log(p)
</script>


</head>
