<title>Insert title here</title>

<script>
	//이름 전화번호 주소만 달라짐.
	
	function makePerson(name, phone, addr){
		let person = {
				'name': name,
				'phone': phone,
				'addr': addr,
				'info': function(){
					let msg=''
					for (let key in this){
						if(key !='info')
						msg+= key + ':' +this[key] +'\n'
					}
					alert(msg)
				}
		}
		return person
	}
	let p = makPerson('홍길동','010-2222-3333','경기도 광명시')
	p.info()

	let persons =[]
	
</script>
