<script>
//상속
	function Person(name, phone, addr) {
		this.name = name
		this.phone = phone
		this.addr = addr
	}

	Person.prototype.hobby = '영화감상'
	Person.prototype.info = function() {
		let msg = ''
		for ( let key in this) {//내객체안의 모든 키를 보고 추출
			if (key != 'info')
				msg += key + ':' + this[key] + '\n'
		}

		alert(msg)
	}
	
	function Doctor(name, phone, addr, major){
		this.super =Person
		this.super(name,phone,addr)
		this.addr=addr
		this.major=major
	}
	
	/*let p = new Person("홍길동", '010-2200-4444', '경기도')
	p.info()
	console.log(p)*/
	
	let d= new Doctor('나최고','010-2222-3333','서울시 서초구','신경외과')
	
	console.log(d)
	console.log("d instanceof Person: "= (d instanceof Person)) // 일부분이야?
	console.log("d instanceof Doctor: "= (d instanceof Doctor)) // 일부분이야?
			
	
</script>


