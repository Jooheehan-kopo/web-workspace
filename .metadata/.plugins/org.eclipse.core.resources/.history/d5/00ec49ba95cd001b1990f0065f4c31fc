<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>

//각 배열의 값에 몇을 더한 값을 만들기
	let nums = [10,20,30,40,50]
	document.write('nums : '+ nums+ '<br>')
	
/*	let copyNums = []
	for (let num of nums){
		copyNums.push(num+100)
	} 
	*/
	
	let copyNums = nums.map(function(element){
		return element+100
	})
	document.write('copyNums: ' +copyNums +'<br>')
	
	nums =[5,23,1,676,34,67,9,32,45,56]
	
	let evenNums = nums.filter(function(element,index,array){
		return element %2== 0  //이 조건을 만족하는 값만 가지고 만듦
	})
	document.write('nums: '+ nums+'<br>')
	document.write('evenNums : '+ evenNums+'<br>')
</script>
