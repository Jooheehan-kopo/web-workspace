<script>
let array = [1,2,3,4,5]
let array2 = array

console.log('array :' + array)
console.log('array2 :' + array)

array2[0]=100
console.log('array :' + array)
console.log('array2 :' + array)




//깊은 복사
let array=[]
for (let i in array){
	array2[i]=array[i]
}
console.log('array :' + array)
console.log('array2 :' + array)

array2[0]=100
console.log('array :' + array)
console.log('array2 :' + array)
</script>
