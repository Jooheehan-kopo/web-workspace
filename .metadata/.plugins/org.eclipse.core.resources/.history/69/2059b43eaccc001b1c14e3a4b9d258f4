<title>Insert title here</title>
<script>

	//선언적 함수 : 이름을 가지고 있음
	
	testFunction()  // 호이스팅 일어나고 있음. 밑의 선언된 것 위에서 불러올 수 있음.
	function testFunction(){
		alert("test fuction 호출..")
	}
	
	function testFunction01(data){
		alert('data: '+data)
	}
	testFunction01(123)
	
	function testFunction02(a,b){
		alert(`a:${a},b:${b}`)
		
	}
	testFunction02('A',10)

	function add(a,b){
		return a+b
	}
	let sum = add(10,20)
	alert('총합: '+sum)
	
	
	
	</script>

