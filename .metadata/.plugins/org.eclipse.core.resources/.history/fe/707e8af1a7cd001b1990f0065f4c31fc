<script>
	window.onload = function() { 
		let tags = document.querySelectorAll('td')
		console.log(tags[0].innerText='아이디')  // id와 pwd 중에서 하나의 값. td의 하나 뽑아서(배열의 첫번째) 고침.
		
		let tags =document.getElementsByTagName('td')  //tag를 통해 찾는 것임. 요소가 여러개일수있음. 그래서 elements임
		console.log(tags)
	}
</script>
</head>
