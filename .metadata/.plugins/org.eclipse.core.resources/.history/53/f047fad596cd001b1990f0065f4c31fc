<script>

//각 배열의 값에 몇을 더한 값을 만들기
	let nums = [10,20,30,40,50]
	document.write('nums : '+ nums+ '<br>')
	
/*	let copyNums = []
	for (let num of nums){
		copyNums.push(num+100)
	} 
	*/
	
	let copyNums = nums.map(function(element){
		return element+100
	})
	document.write('copyNums: ' +copyNums +'<br>')
	
	nums =[5,23,1,676,34,67,9,32,45,56]
	
	let evenNums = nums.filter(function(element,index,array){
		return element %2== 0  //이 조건을 만족하는 값만 가지고 만듦  --filter 
	})
	document.write('nums: '+ nums+'<br>')
	document.write('evenNums : '+ evenNums+'<br>')
	
	let bool= nums.every(function(element){  //모든 조건을 다 만족할때만  bool값을 true를 가짐
		return element>0 
	})
	 document.write('양의정수집합 판단: ' +bool +'<br>')
	 
	 
	 bool= nums.some(function(element){
		 return element %2==0
	 } ) //하나도 만족못하면 거짓임.
	 
	 document.write('짝수포함여부: ' +bool +'<br>')
	 
	 
	 nums = [1,2,3,4,5]
	let sum = 0
	for(const data of nums){
		sum+= data
	}
	 document.write(nums+'총합: ' +sum +'<br>')
	 
	 nums.reduce(function(before,element,index,array){
		 console.log(arguement)
	 })
	 document.write(nums+'총합: ' +sum +'<br>')
</script>
